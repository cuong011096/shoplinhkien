
@model IEnumerable<WebLinhkien.Models.Categories>

@foreach (var c in Model)
{

    <table class="table table-hover" style="margin-bottom:0px!important">


        <tr>
            <td>
                <a href="/Products/Category?CategoryID=@c.ID">
                    <h5 class="col-md-9"><strong>@c.Name</strong></h5>
                </a>
            </td>
        </tr>

    </table>
}

